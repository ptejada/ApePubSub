{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["./js/ApePubSub.js","./APE_Server/APScripts/framework/","./README.md"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "docs"
	},
	"plugins": ["plugins/markdown"],
	"markdown": {
	    "parser": "gfm",
        "githubRepoName": "ApePubSub",
        "githubRepoOwner ": "ptejada"
	},
	"templates": {
		"cleverLinks"     : false,
		"monospaceLinks"  : false,
		"default"         : {
			"outputSourceFiles" : true
		},
		"systemName"      : "ApePubSub",
		"footer"          : "Version 1.6.5",
		"copyright"       : "Pablo Tejada Copyright Â© 2012-2014.",
		"navType"         : "inline",
		"theme"           : "spacelab",
		"linenums"        : true,
		"collapseSymbols" : false,
		"inverseNav"      : true
	},
	"opts": {
		"template": "C:\\Users\\Pablo\\AppData\\Roaming\\npm\\node_modules\\jsdoc\\templates\\docstrap",
		"encoding": "utf8",
		"destination": "docs",
		"recurse": true,
		"private": false
	}
}